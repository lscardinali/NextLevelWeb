		"resend": "^4.1.2",
		"svelte": "^5.19.7",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.6",
		"tailwindcss-safe-area": "^0.6.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0",
		"vite": "^6.0.11",
		"vitest": "^3.0.5"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@neondatabase/serverless": "^0.10.4",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
